{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/tesla/auth",
      "destination": "/api/tesla/auth"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/appspecific/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Content-Type", "value": "text/plain" }
      ]
    }
  ]
}
